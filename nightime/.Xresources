! URxvt (rxvt-unicode) -----------------------------------------

URxvt*termName:             rxvt-256color
URxvt*loginShell:           false
URxvt*saveLines:            1024
URxvt*geometry:             55x15
URxvt*depth:                32
URxvt*font:                 -lucy-tewi-medium-r-normal--11-90-75-75-m-60-iso10646-1
URxvt*boldFont:             -lucy-tewi-medium-r-normal--11-90-75-75-m-60-iso10646-1
!URxvt*font:		xft:Kozuka Gothic Pro M:medium:size=10
!URxvt*boldFont:		xft:Kozuka Gothic Pro M:medium:size=10
URxvt*scrollBar:            false
URxvt*cursorBlink:          false
!URxvt*cursorColor:          #5c4c4c
!URxvt*internalBorder:       20
!URxvt*letterSpace:	-4

Xft.antialias: 1
Xft.dpi: 96
Xtf.hinting: 1
Xft.hinystyle: hintslight
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
